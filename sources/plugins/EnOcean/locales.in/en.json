            },
            "profile": {
              "name": "Profile",
              "description": "Select the device profile, normaly provided on the device label or manual"
            }
          }
        }
      }
    }
  },
  "extraQueries": {
    "pairing": {
      "name": "Start/stop pairing"
    }
