{
  "type": "oledEspEasy",
  "version": "@PLUGIN_VERSION@",
  "author": "Jano42",
  "credits": "",
  "supportedPlatforms": "all",
  "dependencies": {
    "yadoms": {
      "minimumVersion": "2.5.3-beta.1"
    }
  },
  "configurationSchema": {
    "ipAddress": {
      "type": "string",
      "regex": "([0-9]{1,3}\\.){3}[0-9]{1,3}"
    },
    "specificOledSize": {
      "type": "checkboxSection",
      "defaultValue": false,
      "content": {
        "LineCount": {
          "type": "int",
          "defaultValue": 8,
          "minimumValue": 1
        }
      }
    }
  }
}