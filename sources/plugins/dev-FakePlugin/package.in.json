{
  "type": "dev-fakePlugin",
  "version": "@PLUGIN_VERSION@",
  "author": "SÃ©bastien GALLOU",
  "url": "https://github.com/Yadoms/yadoms/wiki/FakePlugin",
  "credits": "",
  "supportedPlatforms": "all",
  "dependencies": {
    "yadoms": {
      "minimumVersion": "2.4.0-beta.1"
    }
  },
  "configurationSchema": {
    "StringParameter": {
      "type": "string"
    },
    "BoolParameter": {
      "type": "bool",
      "defaultValue": "false"
    },
    "IntParameter": {
      "type": "int",
      "defaultValue": "258"
    },
    "DecimalParameter": {
      "type": "decimal",
      "defaultValue": "25.3"
    },
    "EnumParameter": {
      "type": "enum",
      "values": {
        "EnumValue1": "",
        "EnumValue2": "",
        "EnumValue3": ""
      },
      "defaultValue": "EnumValue2"
    },
    "MySection": {
      "type": "section",
      "content": {
        "SubIntParameter": {
          "type": "int",
          "defaultValue": "65535"
        },
        "SubStringParameter": {
          "type": "string"
        }
      }
    },
    "ConditionalParameter": {
      "type": "string",
      "show": {
        "__Binding__": {
          "type": "system",
          "query": "platformIsWindows"
        }
      }
    }
  },
  "supportManuallyDeviceCreation": "true",
  "supportDeviceRemovedNotification": "true",
  "enumerations": {
    "EFakeControllerValues": {
      "name": "",
      "values": {
        "Stop": "",
        "Run": "",
        "Back": "",
        "Left": "",
        "Right": ""
      }
    }
  },
  "recipientFields": {
    "address": {
      "type": "string",
      "regex": ""
    },
    "zipcode": {
      "type": "string",
      "regex": "^(F-)?((2[A|B])|[0-9]{2})[0-9]{3}$"
    },
    "mms": {
      "type": "bool",
      "defaultValue": "true"
    }
  },
  "extraQueries": {
    "simpleCommand": {
      "iconClass": "fa fa-bolt"
    },
    "textDataCommand": {
      "iconClass": "fa fa-edit",
      "commandData": {
        "testValue": {
          "type": "string"
        }
      }
    },
    "numericDataCommand": {
      "iconClass": "fa fa-edit",
      "commandData": {
        "testValue": {
          "type": "int"
        }
      }
    },
    "dataBindingCommand": {
      "iconClass": "fa fa-sitemap",
      "commandData": {
        "networkInterface": {
          "type": "enum",
          "values": {
            "__Binding__": {
              "type": "system",
              "query": "networkInterfaces"
            }
          }
        }
      }
    },
    "dataBindingPluginCommand": {
      "iconClass": "fa fa-rocket",
      "commandData": {
        "dynamicSection": {
          "type": "section",
          "content": {
            "__Binding__": {
              "type": "plugin",
              "query": "test"
            }
          }
        }
      }
    },
    "changePluginStateMessage": {
      "iconClass": "fa fa-recycle",
      "commandData": {
        "newStateMessage": {
          "type": "string"
        }
      }
    },
    "asyncEQwithProgression": {
      "iconClass": "fa  fa-upload",
      "commandData": {
        "fileContent": {
          "type": "file",
          "filter": ".txt"
        }
      }
    }
  },
  "deviceConfiguration": {
    "staticConfigurationSchema": {
      "schemas": {
        "fakeConfigurableDevice": {
          "types": {
            "fakeConfigurableDeviceType": {
              "canBeCreatedManually": "false"
            },
            "anotherFakeConfigurableDeviceType": {
              "canBeCreatedManually": "true"
            }
          },
          "content": {
            "CounterDivider2": {
              "type": "int",
              "defaultValue": "2",
              "minimumValue": "1",
              "maximumValue": "10"
            },
            "MySection": {
              "type": "section",
              "content": {
                "SubIntParameter": {
                  "type": "int",
                  "defaultValue": "65535"
                },
                "SubStringParameter": {
                  "type": "string"
                },
                "SubComboParameter": {
                  "type": "comboSection",
                  "content": {
                    "first": {
                      "name": "First section",
                      "type": "section"
                    },
                    "second": {
                      "name": "Second section",
                      "type": "section"
                    },
                    "third": {
                      "name": "Third section",
                      "type": "section"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "dynamicConfigurationSchema": "true"
  }
}