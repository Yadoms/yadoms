{
  "type": "Sigfox",
  "version": "@PLUGIN_VERSION@",
  "author": "Jean-Michel BERHAULT",
  "url": "https://github.com/Yadoms/yadoms/wiki/Sigfox",
  "credits": "",
  "supportedPlatforms": "all",
  "dependencies": {
    "yadoms": {
      "minimumVersion": "2.3.0-beta.1"
    }
  },
  "configurationSchema": {
    "port": {
      "type": "int",
      "defaultValue": 8090
    },
    "advancedConfiguration": {
      "type": "section",
      "content": {
        "rssiMin": {
          "type": "decimal",
          "defaultValue": -150,
          "step": 1
        },
        "rssiMax": {
          "type": "decimal",
          "defaultValue": -100,
          "step": 1
        },
        "tensionMin": {
          "type": "decimal",
          "defaultValue": 2.4,
          "precision" : 1 
        },
        "tensionMax": {
          "type": "decimal",
          "defaultValue": 3.6,
          "precision": 1
        }
      }
    }
  },
  "supportManuallyDeviceCreation": false,
  "supportDeviceRemovedNotification": false
}