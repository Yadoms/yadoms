             "La Crosse" : { "type" : "bool", "name" : "La Crosse", "defaultValue" : "true" },
             "FS20" : { "type" : "bool", "name" : "FS20", "defaultValue" : "false" },
             "ProGuard" : { "type" : "bool", "name" : "ProGuard", "defaultValue" : "false" },
             "BlindsT0" : { "type" : "bool", "name" : "BlindsT0", "defaultValue" : "false" },
             "BlindsT1" : { "type" : "bool", "name" : "BlindsT1", "defaultValue" : "false" },
             "X10" : { "type" : "bool", "name" : "X10", "defaultValue" : "true" },
             "ARC" : { "type" : "bool", "name" : "ARC", "defaultValue" : "true" },
             "AC" : { "type" : "bool", "name" : "AC", "defaultValue" : "true" },
             "HomeEasy EU" : { "type" : "bool", "name" : "HomeEasy EU", "defaultValue" : "true" },
             "Meiantech" : { "type" : "bool", "name" : "Meiantech", "defaultValue" : "false" },
