   "supportedPlatforms": "all",
   "configurationSchema":
   {
      "SerialPort" :
      {
         "type" : "enum",
         "name" : "Serial port",
         "description" : "The virtual serial port connected to the RFXCom",
		 "values" : {"__Binding__" : {"type" : "system", "query" : "serialPorts"}}
      },
      "Protocols":
      {
          "type" : "section",
          "name" : "Protocols to listen",
          "description" : "Supported protocols to listen, see RFXCom manual to configure. Be careful, some protocols are incompatible and must not be selected together.",
          "enableWithCheckBox" : "false",
          "enableWithCheckBoxDefaultValue" : "false",
          "content" :
          {
             "AE Blyss" : { "type" : "bool", "name" : "AE Blyss", "defaultValue" : "false" },
             "Rubicson" : { "type" : "bool", "name" : "Rubicson", "defaultValue" : "false" },
             "FineOffset/Viking" : { "type" : "bool", "name" : "FineOffset/Viking", "defaultValue" : "false" },
             "Lighting4" : { "type" : "bool", "name" : "Lighting4", "defaultValue" : "false" },
             "RSL" : { "type" : "bool", "name" : "RSL", "defaultValue" : "false" },
             "Byron SX" : { "type" : "bool", "name" : "Byron SX", "defaultValue" : "false" },
             "RFU" : { "type" : "bool", "name" : "RFU", "defaultValue" : "false" },
             "undecoded messages" : { "type" : "bool", "name" : "undecoded messages", "defaultValue" : "false" },
             "Mertik" : { "type" : "bool", "name" : "Mertik", "defaultValue" : "false" },
             "AD LightwaveRF" : { "type" : "bool", "name" : "AD LightwaveRF", "defaultValue" : "false" },
