{
	"type": "chart",
	"version": "0.1",
	"releaseType": "beta",
	"author": "yadoms team",
	"url": "http://www.yadoms.com/wiki/widgets/chart.html",
	"credits": "",
	"dimensions": {
		"min": {
			"x": 3,
			"y": 1
		},
		"max": {
			"x": 9,
			"y": 4
		},
		"default": {
			"x": 3,
			"y": 2
		}
	},
	"configurationSchema": {
		"chartTitle": {
			"type": "string",
			"defaultValue": "",
			"required": "false"
		},
		"interval": {
			"type": "enum",
			"values": {
				"HOUR": "",
				"DAY": "",
				"WEEK": "",
				"MONTH": "",
				"HALF_YEAR": "",
				"YEAR": ""
			},
			"defaultValue": "DAY"
		},
		"oneAxis": {
			"type": "bool",
			"defaultValue": "false"
		},
		"customYAxisMinMax": {
			"type": "section",
			"enableWithCheckBox": "true",
			"enableWithCheckBoxDefaultValue": "false",
			"content": {
				"maximumValue": {
					"type": "decimal",
					"defaultValue": "0.0",
					"precision": "1"
				},
				"minimumValue": {
					"type": "decimal",
					"defaultValue": "0.0",
					"precision": "1"
				}
			}
		},
		"devices": {
			"type": "list",
			"nbItemsMin": "1",
			"nbItemsMax": "3",
			"allowDuplication": "true",
			"item": {
				"type": "section",
				"content": {
					"source": {
						"type": "device",
						"expectedKeywordType": ["numeric",
						"bool"],
						"expectedKeywordAccess": "get"
					},
					"color": {
						"type": "color",
						"defaultValue": "#0000FF"
					},
					"PlotType": {
						"type": "enum",
						"values": {
							"line": "",
							"bar": "",
							"arearange": ""
						},
						"defaultValue": "line"
					}
				}
			}
		}
	},
	"navigator": {
		"hour": "",
		"day": "",
		"week": "",
		"month": "",
		"half_month": "",
		"year": ""
	}
}