{
    "name": "simple-clock",
    "version": "0.1",
    "author": "Nicolas HILAIRE",
    "dimensions": {
        "min": {
            "x": 1,
            "y": 1
        },
        "max": {
            "x": 5,
            "y": 2
        },
        "default": {
            "x": 1,
            "y": 1
        }
    },
    "configurationSchema": {
       "intParameter" : {
          "type" : "int",
          "name" : "int parameter",
          "description" : "test of an int parameter",
          "defaultValue" : "0",
          "maximumValue" : "23"
       },
       "coefficient" : {
          "type" : "decimal",
          "name" : "coeff parameter",
          "description" : "test of an decimal parameter",
          "defaultValue" : "1.23",
          "maximumValue" : "2",
          "minimumValue" : "-2",
          "precision" : "2"
       }
    }
}